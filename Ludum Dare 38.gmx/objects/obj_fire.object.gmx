        worker = instance_nearest(mouse_x,mouse_y,obj_worker);

        if(worker != noone) &amp;&amp; (!ignited) &amp;&amp; (!burning) {
            if(obj_day.seconds &gt;= 230) &amp;&amp; (obj_day.seconds &lt; 360) {   
